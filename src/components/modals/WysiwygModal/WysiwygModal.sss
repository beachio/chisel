.wrapper
  position: absolute
  top: 0
  left: 0
  width: 100%
  height: 100%
  background: white
  z-index: 80
  padding: 50px
  
  .return
    position: absolute
    top: 10px
    left: 10px
    height: 24px
    width: 24px
  
    .cross
      height: 24px
      width: 24px
      opacity: .6
      cursor: pointer

      &:hover
        opacity: 1

  .editor
    width: 100%
    height: 100%
